{
  "compileOnSave": true,
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "sourceMap": false,
    "declaration": false,
    "module": "commonJs",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "jsx": "preserve",
    "lib": [
      "es2018",
    ]
  },
  "include": [ "*.ts" ],
  "exclude": [ "node_modules" ],
}
